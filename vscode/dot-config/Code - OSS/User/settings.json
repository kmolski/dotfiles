{
  "debug.console.fontSize": 16,
  "editor.fontFamily": "'Iosevka', 'Source Code Pro', 'Droid Sans Fallback'",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.highlightActiveIndentGuide": false,
  "editor.renderIndentGuides": false,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontFamily": "Iosevka",
  "terminal.integrated.fontSize": 16,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "window.menuBarVisibility": "default",
  "window.titleBarStyle": "native",
  "workbench.colorTheme": "Solarized Light",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "bracket-pair-colorizer-2.colors": ["Crimson", "Purple", "Chocolate", "Green", "DimGray"],
  "bracket-pair-colorizer-2.forceIterationColorCycle": true,
  "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
  "bracket-pair-colorizer-2.showVerticalScopeLine": false,
  "workbench.startupEditor": "newUntitledFile",
  "window.zoomLevel": 0,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "editor.minimap.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorBlinking": "solid",
  "prettier.printWidth": 100,
  "prettier.tabWidth": 4,
  "haskell.serverExecutablePath": "${HOME}/.ghcup/bin/haskell-language-server-wrapper"
}
